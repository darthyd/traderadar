{
  "name": "trade-radar",
  "version": "0.0.1",
  "description": "A simple, easy to use, and fast radar for traders.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build:linux": "electron-packager ./ trade-radar --platform linux --arch x64 --out build/linux/trade-radar/0.0.1 --app-version 0.0.1 --overwrite",
    "build:linux32": "electron-packager ./ trade-radar --platform linux --arch ia32 --out build/linux32/trade-radar/0.0.1 --app-version 0.0.1 --overwrite",
    "build:mac": "electron-packager ./ trade-radar --platform darwin --arch x64 --icon ./assets/icon.icns --out build/mac/trade-radar/0.0.1 --app-version 0.0.1 --overwrite",
    "build:win": "electron-packager ./ trade-radar --platform win32 --arch x64 --out build/windows/trade-radar/0.0.1 --app-version 0.0.1 --overwrite",
    "build:win32": "electron-packager ./ trade-radar --platform win32 --arch ia32 --out build/windows32/trade-radar/0.0.1 --app-version 0.0.1 --overwrite",
    "build:all": "npm run build:linux && npm run build:linux32 && npm run build:mac && npm run build:win && npm run build:win32"
  },
  "keywords": [
    "Electron",
    "Webview",
    "Chromium",
    "Linux",
    "Initial",
    "Alpha",
    "Trading"
  ],
  "author": "Lucas Oliveira",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.0",
    "electron-packager": "^15.2.0"
  }
}
